<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.smhrd.database.FeedMapper">
	<insert id="writeFeed" parameterType="Feed">
		insert into web_board1 values (num_board.nextval,#{writer},#{filename},#{content},sysdate)
	</insert>
	<select id="totalFeed" resultType="Feed">
		select * from web_board1 order by num
	</select>
	<delete id="deleteFeed">
		delete from web_board1 where num=#{num} 
	</delete>
	<select id="detailFeed" resultType="Feed">
		select * from web_board1 where num=#{num}
	</select>

</mapper>