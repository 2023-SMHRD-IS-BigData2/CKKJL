<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.smhrd.database.MatchMapper">
	
	<insert id="makeMEMA" parameterType="Mercenary_Match">
		INSERT INTO TB_MATCH(MATCH_INDEX.NEXTVAL,#{mm},
		#{date},#{team_index},#{start_hour},#{last_hour},
		#{people_num},#{level},#{writing})
	</insert>
	
	<select id="allMEMA_date" resultType="Mercenary_Match">
	SELECT * FROM VW_MM_FEED;
	</select>


</mapper>