<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.smhrd.database.BoardMapper">

	<insert id="writeFeed" parameterType="Feed">
		insert into TB_FEED values(FEED_INDEX.nextval, #{category}, #{user_index}, #{title}, #{content}, sysdate, #{file}, sysdate, #{like_cnt})
	</insert>

	<select id="allBoard" resultType="Board">
	select * from web_board 
	</select>
	
	<select id="detailBoard" resultType="Board">
	select * from web_board where num=#{num}
	</select>
	

</mapper>